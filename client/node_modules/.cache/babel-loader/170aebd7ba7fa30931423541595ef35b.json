http://localhost:3000\");\n    Socket.on(\"connect\", () => {\n      console.log(`You connected with id : ${Socket.id}`);\n      this.connection = `You connected with id : ${Socket.id}`;\n    });\n    Socket.on(\"message\", function (msg) {\n      console.log(msg);\n      let message = {\n        message: msg,\n      };\n      this.messages.push(message);\n    });\n    this.socket = Socket;\n  },\n  methods: {\n    update(message) {\n      console.log(\"HOLAAAAAAAAAA\");\n      this.message = message;\n    },\n    sendMessage() {\n      this.socket.emit(\"message\", this.message);\n      this.message = \"\";\n    },\n  },\n};\n</script>\n\n<template>\n  <v-container>\n    <ul v-if=\"messages.length !== 0\">\n      <li v-for=\"(message, index) in messages\" :key=\"index\">\n        {{ message.message }}\n      </li>\n    </ul>\n    <v-form @submit=\"sendMessage\">\n      {{ connection }}\n      <v-container>\n        <v-row>\n          <v-col cols=\"12\" sm=\"6\" md=\"3\">\n            <v-text-field\n              v-model=\"message\"\n              label=\"Outlined\"\n              placeholder=\"Placeholder\"\n              outlined\n            ></v-text-field>\n          </v-col>\n        </v-row>\n        <v-btn depressed color=\"primary\" type=\"submit\"> Send </v-btn>\n      </v-container>\n    </v-form>\n  </v-container>\n</template>"]},"metadata":{},"sourceType":"module"}