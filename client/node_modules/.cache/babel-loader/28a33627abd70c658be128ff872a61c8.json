{"ast":null,"code":"import { io } from \"socket.io-client\";\nimport Message from \"../components/MessageChat.vue\";\nexport default {\n  name: \"ChatPage\",\n  components: {\n    Message: Message\n  },\n  data: function data() {\n    return {\n      message: \"Hey!\",\n      items: [{\n        message: \"Foo\"\n      }, {\n        message: \"Bar\"\n      }],\n      socket: {}\n    };\n  },\n  mounted: function mounted() {\n    var _this = this;\n\n    var Socket = io(\"http://localhost:3000\");\n    Socket.on(\"connect\", function () {\n      console.log(\"You connected with id : \".concat(Socket.id));\n    });\n    Socket.on(\"message\", function (msg) {\n      console.log(msg);\n\n      _this.items.push({\n        message: msg\n      });\n    });\n    this.socket = Socket;\n  },\n  methods: {\n    update: function update(message) {\n      console.log(\"HOLAAAAAAAAAA\");\n      this.message = message;\n    },\n    sendMessage: function sendMessage() {\n      this.socket.emit(\"message\", this.message);\n      this.message = \"\";\n    }\n  }\n};","map":{"version":3,"mappings":"AACA;AACA;AAEA;AACAA,kBADA;AAEAC;AACAC;AADA,GAFA;AAKAC,MALA,kBAKA;AACA;AACAC,qBADA;AAEAC;AAAAD;AAAA;AAAAA;AAAA,QAFA;AAGAE;AAHA;AAKA,GAXA;AAYAC,SAZA,qBAYA;AAAA;;AACA;AACAC;AACAC;AACA,KAFA;AAGAD;AACAC;;AACA;AAAAL;AAAA;AACA,KAHA;AAIA;AACA,GAtBA;AAuBAM;AACAC,UADA,kBACAP,OADA,EACA;AACAK;AACA;AACA,KAJA;AAKAG,eALA,yBAKA;AACA;AACA;AACA;AARA;AAvBA","names":["name","components","Message","data","message","items","socket","mounted","Socket","console","methods","update","sendMessage"],"sourceRoot":"src/views","sources":["ChatView.vue"],"sourcesContent":["<script>\nimport { io } from \"socket.io-client\";\nimport Message from \"../components/MessageChat.vue\";\n\nexport default {\n  name: \"ChatPage\",\n  components: {\n    Message,\n  },\n  data() {\n    return {\n      message: \"Hey!\",\n      items: [{ message: \"Foo\" }, { message: \"Bar\" }],\n      socket: {},\n    };\n  },\n  mounted() {\n    var Socket = io(\"http://localhost:3000\");\n    Socket.on(\"connect\", () => {\n      console.log(`You connected with id : ${Socket.id}`);\n    });\n    Socket.on(\"message\", (msg) => {\n      console.log(msg);\n      this.items.push({ message: msg });\n    });\n    this.socket = Socket;\n  },\n  methods: {\n    update(message) {\n      console.log(\"HOLAAAAAAAAAA\");\n      this.message = message;\n    },\n    sendMessage() {\n      this.socket.emit(\"message\", this.message);\n      this.message = \"\";\n    },\n  },\n};\n</script>\n\n<template>\n  <v-container>\n    <v-form @submit=\"sendMessage\">\n      <ul id=\"example-1\">\n        <li v-for=\"item in items\" :key=\"item.message\">\n          {{ item.message }}\n        </li>\n      </ul>\n\n      <v-footer v-bind=\"localAttrs\" :padless=\"padless\">\n        <v-card flat tile width=\"100%\" class=\"red lighten-1 text-center\">\n          <v-card-text>\n            <v-btn v-for=\"icon in icons\" :key=\"icon\" class=\"mx-4\" icon>\n              <v-icon size=\"24px\">\n                {{ icon }}\n              </v-icon>\n            </v-btn>\n          </v-card-text>\n\n          <v-divider></v-divider>\n\n          <v-card-text class=\"white--text\">\n            {{ new Date().getFullYear() }} â€” <strong>Vuetify</strong>\n          </v-card-text>\n        </v-card>\n      </v-footer>\n    </v-form>\n    <script>\n      export default {\n        data: () => ({\n          icons: [\"mdi-home\", \"mdi-email\", \"mdi-calendar\", \"mdi-delete\"],\n          items: [\"default\", \"absolute\", \"fixed\"],\n          padless: false,\n          variant: \"default\",\n        }),\n        computed: {\n          localAttrs() {\n            const attrs = {};\n\n            if (this.variant === \"default\") {\n              attrs.absolute = false;\n              attrs.fixed = false;\n            } else {\n              attrs[this.variant] = true;\n            }\n            return attrs;\n          },\n        },\n      };\n    </script>\n    <Message></Message>\n  </v-container>\n</template>"]},"metadata":{},"sourceType":"module"}