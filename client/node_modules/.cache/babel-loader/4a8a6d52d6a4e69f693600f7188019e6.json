{"ast":null,"code":"import { io } from \"socket.io-client\";\nimport MessagesChat from \"../components/Chat/ChatContainer.vue\";\nexport default {\n  name: \"ChatPage\",\n  components: {\n    MessagesChat: MessagesChat\n  },\n  data: function data() {\n    return {\n      message: \"\",\n      chat: [],\n      socket: {},\n      username: \"You\",\n      numUsers: 0,\n      valid: false,\n      messageRules: [function (v) {\n        return !!v || \"Message can't be empty\";\n      }]\n    };\n  },\n  mounted: function mounted() {\n    var _this = this;\n\n    var Socket = io(\"http://localhost:3000\");\n    Socket.on(\"connect\", function () {\n      console.log(\"You connected with id : \".concat(Socket.id));\n    });\n    Socket.on(\"message\", function (msg) {\n      _this.chat.push(msg);\n    }); //Login\n    //Add new user to the chat rooom\n\n    this.socket = Socket;\n  },\n  methods: {\n    sendMessage: function sendMessage() {\n      if (this.message != \"\") {\n        this.socket.emit(\"message\", this.message);\n        this.chat.push({\n          username: this.username,\n          message: this.message\n        });\n        this.message = \"\";\n      }\n    },\n    typing: function typing() {}\n  }\n};","map":{"version":3,"mappings":"AACA;AACA;AAEA;AACAA,kBADA;AAEAC;AACAC;AADA,GAFA;AAKAC,MALA,kBAKA;AACA;AACAC,iBADA;AAEAC,cAFA;AAGAC,gBAHA;AAIAC,qBAJA;AAKAC,iBALA;AAMAC,kBANA;AAOAC;AAAA;AAAA;AAPA;AASA,GAfA;AAgBAC,SAhBA,qBAgBA;AAAA;;AACA;AACAC;AACAC;AACA,KAFA;AAGAD;AACA;AACA,KAFA,EALA,CAQA;AACA;;AAEA;AACA,GA5BA;AA6BAE;AACAC,eADA,yBACA;AACA;AACA;AACA;AAAAR;AAAAH;AAAA;AACA;AACA;AACA,KAPA;AAQAY,UARA,oBAQA;AARA;AA7BA","names":["name","components","MessagesChat","data","message","chat","socket","username","numUsers","valid","messageRules","mounted","Socket","console","methods","sendMessage","typing"],"sourceRoot":"src/views","sources":["ChatView.vue"],"sourcesContent":["<script>\nimport { io } from \"socket.io-client\";\nimport MessagesChat from \"../components/Chat/ChatContainer.vue\";\n\nexport default {\n  name: \"ChatPage\",\n  components: {\n    MessagesChat,\n  },\n  data() {\n    return {\n      message: \"\",\n      chat: [],\n      socket: {},\n      username: \"You\",\n      numUsers: 0,\n      valid: false,\n      messageRules: [(v) => !!v || \"Message can't be empty\"],\n    };\n  },\n  mounted() {\n    var Socket = io(\"http://localhost:3000\");\n    Socket.on(\"connect\", () => {\n      console.log(`You connected with id : ${Socket.id}`);\n    });\n    Socket.on(\"message\", (msg) => {\n      this.chat.push(msg);\n    });\n    //Login\n    //Add new user to the chat rooom\n\n    this.socket = Socket;\n  },\n  methods: {\n    sendMessage() {\n      if (this.message != \"\") {\n        this.socket.emit(\"message\", this.message);\n        this.chat.push({ username: this.username, message: this.message });\n        this.message = \"\";\n      }\n    },\n    typing() {},\n  },\n};\n</script>\n\n<template>\n  <v-container>\n    <h1>Register as {{ username }}</h1>\n    <MessagesChat :chat.sync=\"chat\"></MessagesChat>\n    <v-progress-linear\n      color=\"red lighten-2\"\n      buffer-value=\"0\"\n      stream\n    ></v-progress-linear>\n    <v-form ref=\"form\" v-model=\"valid\" lazy-validation @submit=\"sendMessage\">\n      <v-footer padless absolute>\n        <v-container>\n          <v-row>\n            <v-text-field\n              v-model=\"message\"\n              label=\"Send a Message\"\n              placeholder=\"Aa\"\n              outlined\n              required\n            ></v-text-field>\n            <v-btn depressed color=\"primary\" type=\"submit\"> Send </v-btn>\n          </v-row>\n        </v-container>\n        <v-col class=\"text-center\" cols=\"12\"> </v-col>\n      </v-footer>\n    </v-form>\n  </v-container>\n</template>\n\n"]},"metadata":{},"sourceType":"module"}