h id : ${Socket.id}`);\n    });\n    Socket.on(\"message\", (msg) => {\n      console.log(msg);\n      this.chat.push({ message: msg });\n    });\n    //Login\n    Socket.on(\"login\", (username, numUsers) => {\n      this.user = username;\n      this.numUsers = numUsers;\n    });\n    this.socket = Socket;\n  },\n  methods: {\n    sendMessage() {\n      this.socket.emit(\"message\", this.message);\n      //      var msg = { message: this.message, user: this.username };\n\n      this.message = \"\";\n    },\n    typing() {},\n  },\n};\n</script>\n\n<template>\n  <v-container>\n    <h1>Registrado como : {{ username }}</h1>\n    <v-form @submit=\"sendMessage\">\n      <ul id=\"example-1\">\n        <li v-for=\"item in chat\" :key=\"item.username\">\n          {{ item.username }} {{ item.message }}\n        </li>\n      </ul>\n\n      <v-footer padless absolute>\n        <v-container>\n          <v-row>\n            <v-text-field\n              v-model=\"message\"\n              label=\"Send a Message\"\n              placeholder=\"Aa\"\n              outlined\n              required\n            ></v-text-field>\n            <v-btn depressed color=\"primary\" type=\"submit\"> Send </v-btn>\n          </v-row>\n        </v-container>\n        <v-col class=\"text-center\" cols=\"12\"> </v-col>\n      </v-footer>\n    </v-form>\n  </v-container>\n</template>"]},"metadata":{},"sourceType":"module"}